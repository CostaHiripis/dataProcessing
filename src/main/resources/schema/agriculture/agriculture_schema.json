{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "The value of items produced in the respective agricultural sector",
  "type": "object",
  "properties": {
    "areaCode":{
      "type": "number",
      "maxItems": 1,
      "minimum": 1
    },
    "area": {
      "type": "string",
      "maxItems": 1
    },
    "itemCode": {
      "type": "number",
      "maxItems": 1,
      "minimum": 1
    },
    "item": {
      "type": "string",
      "maxItems": 1
    },
    "elementCode": {
      "type": "number",
      "maxItems": 1,
      "minimum": 1,
      "pattern": "^[0-9]{4}$"
    },
    "element": {
      "type": "string",
      "maxItems": 1
    },
    "year": {
      "type": "number",
      "maxItems": 1,
      "minimum": 1900,
      "maximum": 2020
    },
    "unit": {
      "type": "string",
      "maxItems": 1,
      "enum": [
        "0.1g/An",
        "1000 Head",
        "1000 No",
        "100mg/An",
        "ha",
        "Head",
        "hg",
        "hg/An",
        "hg/ha",
        "No",
        "No/An",
        "tonnes"
      ]
    },
    "value": {
      "type": "number",
      "maxItems": 1,
      "minimum": 0
    },
    "flag": {
      "type": "string",
      "maxItems": 1,
      "enum": [
        "*",
        "A",
        "F",
        "Fc",
        "lm",
        "M",
        "R",
        ""
      ]
    }
  }
}