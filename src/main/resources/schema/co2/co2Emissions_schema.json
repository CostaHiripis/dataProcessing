{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "The CO2 emissions produced per country each year",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "maxItems": 1,
      "minimum": 1
    },
    "countryCode": {
      "type": "string",
      "maxItems": 1,
      "pattern": "^[A-Z]{3}$"
    },
    "country": {
      "type": "string",
      "maxItems": 1
    },
    "year": {
      "type": "number",
      "maxItems": 1,
      "minimum": 1900,
      "maximum": 2020
    },
    "value": {
      "type": "number",
      "maxItems": 1
    }
  }
}